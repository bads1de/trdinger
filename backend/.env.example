# アプリケーション設定
APP_NAME=Trdinger Trading API
APP_VERSION=1.0.0
DEBUG=false

# サーバー設定
HOST=127.0.0.1
PORT=8000

# CORS設定
CORS_ORIGINS=["http://localhost:3000"]

# データベース設定
DATABASE_URL=postgresql://postgres:password@localhost:5432/trdinger
DB_HOST=localhost
DB_PORT=5432
DB_NAME=trdinger
DB_USER=postgres
DB_PASSWORD=your_password_here

# ログ設定
LOG_LEVEL=INFO
LOG_FORMAT="%(asctime)s - %(name)s - %(levelname)s - %(message)s"

# セキュリティ設定
SECRET_KEY=your-secret-key-here
